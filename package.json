{
  "version": "1.0.0",
  "name": "mued-project",
  "description": "A React Learning Management System project using Next.js App Router with Shadcn UI library",
  "scripts": {
    "dev": "next dev --port 3000 --hostname 0.0.0.0",
    "build": "npx prisma generate && next build",
    "export": "next export",
    "build:static": "bash scripts/vercel-deploy-prep.sh && npx prisma generate && next build && next export",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx}\"",
    "test": "vitest",
    "test:ci": "vitest run --coverage",
    "test:api": "vitest run app/api/**/*.test.ts",
    "test:api:coverage": "vitest run --coverage app/api/**/*.test.ts",
    "test:lessons": "vitest run app/api/lesson-slots/__tests__/route.test.ts",
    "test:reservations": "vitest run app/api/reservations/__tests__/route.test.ts",
    "test:coverage": "vitest run --coverage",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "gen-env": "npx tsx scripts/gen-env.ts",
    "check-env": "node scripts/check-env.js",
    "seed": "npx tsx scripts/seed-test-db.ts",
    "test:e2e": "playwright test",
    "test:stripe-webhook": "npx tsx scripts/test-stripe-webhook.ts",
    "check:supabase-permissions": "npx tsx scripts/check-supabase-permissions.ts",
    "add-dynamic-flags": "node scripts/add-dynamic-flag.js"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.9.0",
