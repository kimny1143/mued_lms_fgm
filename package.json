    "idb": "^8.0.2",
    "lucide-react": "^0.453.0",
    "next": "^14.2.3",
    "next-themes": "^0.4.6",
    "react": "^18.2.0",
    "react-day-picker": "^9.7.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.56.1",
    "react-responsive": "^10.0.1",
