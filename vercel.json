{
  "buildCommand": "npm run build",
  "outputDirectory": "out",
  "git": {
    "deploymentEnabled": {
      "main": true,
      "develop": true,
      "feature/sprint-R0": true
    }
  },
  "github": {
    "enabled": true,
    "silent": false
  },
  "framework": "nextjs",
  "rewrites": [
    { "source": "/(.*)", "destination": "/" }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/" }
  ]
} 