{
  "name": "@mued/web",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "npm run kill-port && npm run kill-mcp-port && next dev --port 3000 --hostname 0.0.0.0",
    "dev:secure": "npm run kill-port && npm run kill-mcp-port && next dev --port 3000 --hostname localhost",
    "dev:mobile": "MOBILE_PREVIEW=true npm run dev",
    "dev:tunnel": "ngrok http 3000",
    "build": "npx prisma generate && next build",
    "export": "next export",
    "build:static": "bash scripts/vercel-deploy-prep.sh && npx prisma generate && next build && next export",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx}\"",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ci": "vitest run --coverage",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "kill-port": "lsof -ti:3000 | xargs kill -9 || true",
    "kill-mcp-port": "lsof -ti:3333 | xargs kill -9 || true",
    "setup:local": "cp .env.local.example .env.local && echo 'Please update .env.local with your actual values'"
  },
