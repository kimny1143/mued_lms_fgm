{
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "framework": null,
  "installCommand": "npm install",
  "devCommand": "npm start",
  "ignoreCommand": "npx turbo-ignore @mued/mobile",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://mued-lms-fgm-git-develop-glasswerks.vercel.app/api/:path*"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ]
}